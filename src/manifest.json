{
  "content_scripts": [{
    "js": [
      "VanillaTimeline.js"
    ],
    "matches": [ 
      "https://twitter.com/*"
     ],
    "include_globs":  ["https://twitter.com/home", "https://twitter.com/", "https://twitter.com"]
  }],
  "permissions": [
    "storage"
  ],
  "icons": { 
    "16": "icon-16.png",    
    "32": "icon-32.png",    
    "64": "icon-64.png",
    "128": "icon-128.png"
  },
  "name": "Vanilla Timeline",
  "version": "1.3",
  "author": "Jacob Pretorius",
  "description": "For those of us who like social media less noisy.",
  "homepage_url": "https://github.com/jacobpretorius/Vanilla-Timeline",
  "manifest_version": 2,
  "options_page": "settings.html"
}
